actionTypes.AUTH_RESET):\r\n            return {\r\n                ...state,\r\n                tasks : [],\r\n                buckets : [],\r\n                editing : false,\r\n                \r\n                auth : {\r\n                    ...state.auth,\r\n                    username : null,\r\n                    tokenId : null,\r\n                    error : null,\r\n                    loading : false\r\n                }\r\n            }\r\n        case(actionTypes.AUTH_START):\r\n            return {\r\n                ...state,\r\n                auth : {\r\n                    ...state.auth,\r\n                    loading : true\r\n                }\r\n            }\r\n        case(actionTypes.AUTH_SUCCESS):\r\n            return {\r\n                ...state,\r\n                auth : {\r\n                    ...state.auth,\r\n                    loading : false,\r\n                    username : action.username ? action.username : null,\r\n                    tokenId : action.tokenId ? action.tokenId : null,\r\n                    error : null\r\n                }\r\n            }\r\n        case(actionTypes.AUTH_FAIL):\r\n            return {\r\n                ...state,\r\n                auth : {\r\n                    ...state.auth,\r\n                    loading : false,\r\n                    error : action.error\r\n                }\r\n            }\r\n        case(actionTypes.AUTH_LOGOUT):\r\n            return {\r\n                ...state,\r\n                auth : {\r\n                    ...state.auth,\r\n                    tokenId : null,\r\n                    username : null,\r\n                    loading : false,\r\n                    error : null\r\n                }\r\n            }\r\n    }\r\n    return state\r\n}\r\n\r\nexport default reducer","C:\\Users\\jolly9\\Desktop\\react_project\\todo-list\\src\\store\\action\\todo.js",[],"C:\\Users\\jolly9\\Desktop\\react_project\\todo-list\\src\\store\\action\\actionTypes.js",[],"