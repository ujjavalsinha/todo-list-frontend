sole.log(\"EDITING VALUEEEEEE-----\",this.props.editing)\n    let editForm = null\n    if(this.props.editing){\n      editForm = (\n        <div className='BackdropEditForm'>\n          <Route path='/' exact component={Backdrop}/>  \n          <Route path='/' exact component={EditForm}/>      \n        </div>\n      )\n    }\n    return (\n      <div>\n        <NavBar/>\n        <Route path='/login' component={LoginForm}/>\n        <Route path='/signup' component={SignupForm}/>\n        <Route path='/logout' component={Logout}/>\n        {editForm}\n        {this.props.isAuthenticated ?\n        <div className=\"App\">\n        \n        <Route path='/' exact component={Header}/>\n        {!this.props.loading ?\n        <Route path='/' exact component={TodoList}/>\n        :\n        <Route path='/' exact component={Spinner}/>\n        }\n        <Route path='/' exact component={TodoForm}/>\n        </div>\n        :\n        <Redirect to='/signup' />\n        } \n      </div>\n    );\n  }\n  \n}\nconst mapStateToProps = state =>{\n  return {\n    todoItems : state.tasks,\n    loading : state.loading,\n    status_changed : state.status_changed,\n    editing : state.editing,\n    isAuthenticated : state.auth.tokenId !== null,\n    username : state.auth.username\n  }\n}\n\nconst mapDispatchToProps = dispatch => {\n  return {\n    onFetchTodoItems : (username) => dispatch(actions.fetchTodoItems(username)),\n    onFetchingBuckets : (username) => dispatch(bucketActions.fetchBuckets(username))\n  }\n}\nexport default connect(mapStateToProps,mapDispatchToProps)(App);\n","C:\\Users\\jolly9\\Desktop\\react_project\\todo-list\\src\\components\\NavBar\\NavBar.js",[],"C:\\Users\\jolly9\\Desktop\\react_project\\todo-list\\src\\components\\Header\\Header.js",[],"C:\\Users\\jolly9\\Desktop\\react_project\\todo-list\\src\\components\\TodoList\\TodoList.js",["89"],"C:\\Users\\jolly9\\Desktop\\react_project\\todo-list\\src\\components\\TodoItem\\TodoItem.js",[],"C:\\Users\\jolly9\\De